{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-ts-ssr-starter",
  "private": true,
  "version": "0.5.3",
  "description": "Vue2 SSR TS Starter project for Vite.",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "pro": "cross-env NODE_ENV=production node server",
    "clean": "rimraf ./node_modules/.vite dist",
    "build": "pnpm build:client && pnpm build:server",
    "build:client": "vue-tsc --noEmit && vite build --outDir dist/client --ssrManifest",
    "build:server": "vue-tsc --noEmit && vite build --outDir dist/server --ssr src/entry-server.js",
    "build:analyze": "vue-tsc --noEmit && vite build --outDir dist/client --ssrManifest --mode analyze && vite build --outDir dist/server --ssr src/entry-server.js --mode analyze",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^8.9.4",
    "axios": "0.18.1",
    "element-ui": "2.13.2",
    "express": "^4.18.1",
    "lingman-web": "^1.0.1681367236",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
    "pinia": "^2.0.34",
    "serve-static": "^1.15.0",
    "vue": "^2.7.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "3.6.5",
    "vue-server-renderer": "^2.7.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@djie/eslint-config": "^1.0.24",
    "@types/express": "^4.17.13",
    "@types/node": "^18.15.11",
    "@types/webfontloader": "^1.6.34",
    "@typescript-eslint/parser": "^5.30.5",
    "@unocss/reset": "^0.51.12",
    "@vitejs/plugin-vue2": "^2.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "1.26.8",
    "typescript": "^4.7.4",
    "unocss": "^0.51.12",
    "unplugin-auto-import": "0.9.2",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.19.9",
    "vite": "^4.2.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-template-compiler": "^2.7.4",
    "vue-tsc": "^1.4.2"
  }
}